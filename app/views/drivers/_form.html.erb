<%= form_with model: @driver do |f| %>

  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <div>
    <%= f.label :completed_at %>
    <%= f.text_field :completed_at %>
  </div>

  <%= f.submit %>


<% end %>
